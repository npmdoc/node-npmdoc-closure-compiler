    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-closure-compiler.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
